<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" maxHeight="500.0" maxWidth="500.0" minHeight="287.0" minWidth="401.0" prefHeight="287.0" prefWidth="401.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iaroslaveremeev.controllers.StudentController">
   <children>
      <TextField fx:id="textFieldName">
         <VBox.margin>
            <Insets left="20.0" right="20.0" top="50.0" />
         </VBox.margin>
      </TextField>
      <TextField fx:id="textFieldAge">
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
      </TextField>
      <TextField fx:id="textFieldNumber">
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
      </TextField>
      <TextField fx:id="textFieldSalary">
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
      </TextField>
      <Button mnemonicParsing="false" onAction="#updateStudentData" text="Update student data" />
   </children>
</VBox>
